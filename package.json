{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open --port 3002",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": " npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "tsc && vite build",
    "start": "npm run dev",
    "start-remote": "vite --host",
    "test": "playwright test",
    "generate-screenshots": "node screenshots.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/msal-browser": "^3.20.0",
    "@fluentui/web-components": "^2.5.16",
    "@google/generative-ai": "^0.2.0",
    "@langchain/community": "^0.2.10",
    "@mlc-ai/web-llm": "^0.2.52",
    "@shoelace-style/shoelace": "^2.14.0",
    "@thepassle/app-tools": "^0.9.12",
    "@vaadin/router": "^1.7.4",
    "@xenova/transformers": "^2.17.2",
    "browser-fs-access": "^0.35.0",
    "firebase": "^10.12.4",
    "idb-keyval": "^6.2.1",
    "langchain": "^0.2.5",
    "lit": "^3.1.2",
    "marked": "^12.0.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.33.1",
    "minisearch": "^6.3.0",
    "onnxruntime-web": "^1.18.0",
    "speech-to-text-toolkit": "^0.0.7",
    "urlpattern-polyfill": "^10.0.0",
    "web-ai-toolkit": "^0.1.6",
    "workbox-build": "^6.5.4",
    "workbox-core": "^6.5.2",
    "workbox-expiration": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/node": "^20.12.12",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-pwa": "^0.17.5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
